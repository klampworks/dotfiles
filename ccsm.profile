[animation]
s0_open_effects = animationaddon:Beam Up;animationaddon:Burn;animation:Fade;
s0_open_durations = 500;500;150;
s0_close_effects = animationaddon:Explode;animationaddon:Burn;animation:Fade;
s0_close_durations = 600;500;150;

[core]
as_active_plugins = core;ccp;move;resize;place;decoration;regex;commands;imgjpeg;animation;wobbly;animationaddon;wallpaper;cube;scale;3d;rotate;
as_audible_bell = false
as_autoraise = false

[commands]
as_command0 = sakura
as_run_command0_edge = TopLeft

[scale]
as_initiate_all_edge = BottomRight
as_show_desktop = false

[wallpaper]
s0_bg_image = /home/user/Downloads/wall.jpg;
s0_bg_image_pos = 0;
s0_bg_fill_type = 0;
s0_bg_color1 = #000000ff;
s0_bg_color2 = #000000ff;

[3d]
s0_manual_only = false

[decoration]
as_command = emerald --replace

[rotate]
as_edge_flip_pointer = true
as_initiate_button = <TopEdge>Button1
as_rotate_left_key = <Control><Primary><Alt>z
as_rotate_right_key = <Control><Primary><Alt>x
as_rotate_flip_left_edge = Left
as_rotate_flip_right_edge = Right

